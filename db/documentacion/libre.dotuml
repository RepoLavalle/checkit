SequenceDiagram{
  lifeline web 
  lifeline ser as "server"
  lifeline seg as "seguiridad"
  lifeline con as "controlador"
  lifeline mod as "modelo"

 web --> ser 'post/login'
ser --> seg 'validarUsuario(req.body)'
seg --> mod 'nomUsuExiste(data)'
seg <-r- mod 'true'
seg --> mod 'obtenerUsuario(data.user)'
